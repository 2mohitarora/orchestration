{
  "Node": "httpforever",
  "Address": "httpforever.com",
  "NodeMeta": {
    "external-node": "true",
    "external-probe": "true"
  },
  "Service": {
    "ID": "httpforever",
    "Service": "httpforever-svc",
    "Port": 80
  },
  "Checks": [
    {
      "Name": "http-check",
      "status": "passing",
      "Definition": {
        "http": "http://httpforever.com/",
        "interval": "30s"
      }
    }
  ]
}
